    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.3.0",
    "i18next-locales-sync": "^2.1.0",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
